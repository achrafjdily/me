{

  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "generate-resume",
    "outDir": "./compiled-generator",
    "module": "CommonJS",
    "types": ["node"],
    "strict": false,
    "paths": {
      "@store/*": ["app/store/*"],
      "@data/*": ["app/data/*"]
    }
  },
  "files": ["./generate-resume/generate.ts"],
  "exclude": ["src"],
}
